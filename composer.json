{
    "name": "sertxudeveloper/livewire-combobox",
    "description": "Add combobox to your Livewire forms",
    "keywords": [
        "sertxudeveloper",
        "laravel",
        "livewire-combobox"
    ],
    "homepage": "https://github.com/sertxudeveloper/livewire-combobox",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergio Peris",
            "email": "dev.sertxu@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "livewire/livewire": "^3.1",
        "illuminate/contracts": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.5|^8.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "SertxuDeveloper\\LivewireCombobox\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SertxuDeveloper\\LivewireCombobox\\Tests\\": "tests",
            "SertxuDeveloper\\LivewireCombobox\\Tests\\Database\\Factories\\": "tests/database/factories",
            "SertxuDeveloper\\LivewireCombobox\\Tests\\Database\\Seeders\\": "tests/database/seeders"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SertxuDeveloper\\LivewireCombobox\\ComboboxServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
